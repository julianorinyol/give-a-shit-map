<script type="text/javascript">

    $(function() {

        var map = kartograph.map('#map');

        map.loadMap('usa.svg', function() {
            console.log("start of loadmap callback")
            map.addLayer('layer0', {
                styles: {
                    stroke: '#959595',
                    fill: '#303030'
                },
                mouseenter: function(d, path) {
                    path.attr('fill', Math.random() < 0.5 ? '#67D8B6' : '#DD88DD');
                },
                click: function(d, path) {
                    console.log(d)
                    console.log(d["key"])
                    console.log(d["label"])
                    $("#superinfo").html("<p>Hello " + d["label"] + "</p>")
                },
                mouseleave: function(d, path) {
                    path.animate({ fill: '#303030' }, 200);
                }
            
            });

            var points_of_interest = [
                { name: 'Juneau, AK', lat: 58.3, lon: -134.416667 },
                { name: 'Honolulu, HI', lat: 21.3, lon: -157.816667 },
                { name: 'San Francisco, CA', lat: 37.783333, lon: -122.416667 },
            ];

            map.addSymbols({
                type: kartograph.LabeledBubble,
                data: points_of_interest,
                location: function(d) { return [d.lon, d.lat] },
                title: function(d) { return d.name; },
                radius: 3,
                center: false,
                attrs: { fill: 'black' },
                labelattrs: { 'font-size': 11 },
                buffer: true
            });

        }, { padding: 30 });
    });

</script>

    <div id="map"></div>


    <div class = "infocontainer">
        <div id="superinfo"><p>hey</p></div>
    </div>

